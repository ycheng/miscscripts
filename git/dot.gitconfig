[alias]
    info = cat-file commit HEAD
    br = branch
    ci = commit
    co = checkout
    df = diff --ignore-all-space --no-prefix
    lg = log -p
    st = status
    who = shortlog -s --
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative

